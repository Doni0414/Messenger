   <children>
      <TextField fx:id="nameField" layoutX="150.0" layoutY="166.0" onAction="#sendName" />
      <Label layoutX="158.0" layoutY="127.0" text="Enter your name" textFill="WHITE">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
      <Button fx:id="enterButton" layoutX="312.0" layoutY="166.0" mnemonicParsing="false" onAction="#sendName" text="Enter" />
   </children>
</AnchorPane>
